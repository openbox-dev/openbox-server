{
  "name": "open-box",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "commit": "commit",
    "build": "vite build && vite build --ssr",
    "dev": "node ./server.mjs",
    "start": "cross-env NODE_ENV=production node ./server.mjs",
    "typecheck": "tsc",
    "prepare": "husky install | chmod ug+x .husky/*",
    "build:css": "node-sass ./scss -o ./app/styles --output-style compressed",
    "dev:css": "npm run build:css && node-sass -rw ./scss -o ./app/styles"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@remix-run/express": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "express": "^4.18.2",
    "framer-motion": "^10.16.5",
    "isbot": "^3.6.8",
    "node-sass": "^9.0.0",
    "prisma": "^5.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-validated-form": "^5.1.5",
    "sass": "^1.69.5",
    "three": "^0.158.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@commitlint/prompt-cli": "^18.2.0",
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "@types/express": "^4.17.20",
    "@types/node": "^20.9.4",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}